<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="API Regression Suite" verbose="4" preserve-order="true" parallel="none">

	<listeners>
		<listener class-name="io.qameta.allure.testng.AllureTestNg" />
	</listeners>

	<listeners>
		<listener class-name="com.aventstack.chaintest.plugins.ChainTestListener" />
	</listeners>


	<test name="GoRest API Tests">
		<packages>
			<package name="com.qa.api.gorest.tests" />
		</packages>
	</test>

</suite>